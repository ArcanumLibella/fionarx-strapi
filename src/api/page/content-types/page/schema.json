    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.text",
        "blocks.link",
        "blocks.quote",
        "blocks.separator",
        "blocks.spacer",
        "blocks.list",
        "blocks.button",
        "blocks.prestations",
        "blocks.stack",
        "blocks.foot-note",
        "blocks.pricing-card",
        "blocks.pricings",
        "blocks.stage",
        "blocks.stages",
        "blocks.section-cta"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "introduction": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.introduction"
    },
    "sectionCTA": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.section-cta"
    }
  }
